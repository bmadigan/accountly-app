{
    "extends": ["prettier", "plugin:vue/recommended"],
    "plugins": ["vue", "prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "semi": 1,
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "es5",
        "useTabs": false,
        "no-console": "off",
        "no-unused-vars": "warn",
        "func-names": "off"
    }
}
